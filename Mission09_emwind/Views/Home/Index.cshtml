@{
}

@model List<Book>

<h1>Books</h1>
<br />

@foreach (Book b in Model)
{
    <h3>Title: @b.Title</h3>
    <p>Author: @b.Author</p>
    <p>Publisher: @b.Publisher</p>
    <p>ISBN: @b.Isbn</p>
    <p>Classification: @b.Classification</p>
    <p>Category: @b.Category</p>
    <p>Page Count: @b.PageCount</p>
    <p>Price: $@b.Price</p>
    <br />
}
